{
  "plugins": [
    ["component", [
      { "libraryName": "element-ui", "styleLibraryName": "theme-chalk" }
    ]],
    "transform-decorators-legacy",
    "transform-class-properties",
    "syntax-dynamic-import",
    "transform-object-rest-spread"
  ],
  "presets": [
    [ "env", {
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "not ie <= 8"
        ]
      }
    }]
  ]
}
